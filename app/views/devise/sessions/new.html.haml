%h2 Sign in
= form_for("admin", :url => admin_session_path) do |f|
  = f.label :email
  = f.email_field :email, :autofocus => true
  = f.label :password
  = f.password_field :password
  - if devise_mapping.rememberable?
    = f.check_box :remember_me
    = f.label :remember_me
  = f.submit "Sign in"